                            <form action="/admin/review/edit/<%=doc._id%>" method="post">
								<div class="card-body">
									<div class="card-content">
										<p><strong>Paper Title:</strong><%=doc.title%></p>
										<div class="text-right">
											<a href="http://" class="btn btn-primary "><%=doc.fileName%></a>
										</div>
									</div>
									<hr>
									<div id="smartwizard" class="form-wizard order-create">
										<ul class="nav nav-wizard">
											<li><a class="nav-link" href="#wizard_Service"> 
												<span>1</span> 
											</a></li>
											<li><a class="nav-link" href="#wizard_PAPER_QUALITY_ASSESSMENT">
												<span>2</span>
											</a></li>
